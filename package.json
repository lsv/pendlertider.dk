{
  "name": "pendlertider.dk",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext \".js,.vue,.ts\" --ignore-path .gitignore .",
    "lint:style": "stylelint \"**/*.{vue,css,scss,sass}\" --ignore-path .gitignore",
    "lint": "npm run lint:js && npm run lint:style",
    "fix:js": "eslint --fix --ext \".js,.vue,.ts\" --ignore-path .gitignore .",
    "fix:style": "stylelint --fix \"**/*.{vue,css,scss,sass}\" --ignore-path .gitignore",
    "fix": "npm run fix:js && npm run fix:style",
    "test": "jest"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint",
    "*.{css,vue}": "stylelint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run test"
    }
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.12.5",
    "@nuxtjs/pwa": "^3.3.5",
    "core-js": "^3.8.3",
    "luxon": "^1.26.0",
    "nuxt": "^2.15.3",
    "nuxt-buefy": "^0.4.4",
    "nuxt-i18n": "^6.21.0",
    "nuxt-property-decorator": "^2.9.1",
    "vue-geolocation-api": "^0.1.21",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@nuxt/types": "^2.14.12",
    "@nuxt/typescript-build": "^2.0.4",
    "@nuxtjs/color-mode": "^2.0.5",
    "@nuxtjs/eslint-config-typescript": "^5.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/stylelint-module": "^4.0.0",
    "@types/jest": "^26.0.20",
    "@types/luxon": "^1.26.2",
    "@vue/test-utils": "^1.1.2",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.8.0",
    "fibers": "^5.0.0",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "sass": "^1.32.8",
    "sass-loader": "^10.1.1",
    "stylelint": "^13.9.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^20.0.0",
    "ts-jest": "^26.4.4",
    "typescript": "^4.2.3",
    "vue-jest": "^3.0.4"
  }
}
